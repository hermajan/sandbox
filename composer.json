{
	"name": "hermajan/sandbox",
	"description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
	"type": "project",
	"license": "MIT",
	"homepage": "https://hermajan.net/sandbox",
	"support": {
		"issues": "https://github.com/hermajan/sandbox/issues"
	},
	"config": {
		"vendor-dir": "vendor",
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"minimum-stability": "stable",
	"require": {
		"php": "^7.4",
		"doctrine/dbal": "^2.10",
		"doctrine/orm": "^2.7",
		"hermajan/dobine": "^0.3.0",
		"hermajan/fazette": "^0.3.0",
		"latte/latte": "^2.8",
		"nette/nette": "^3.0",
		"tracy/tracy": "^2.7",
		"vlucas/phpdotenv": "^5.1"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"phpstan/phpstan": "^0.12.33",
		"phpstan/phpstan-doctrine": "^0.12.19",
		"phpstan/phpstan-nette": "^0.12.8"
	}
}
