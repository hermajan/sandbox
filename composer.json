{
	"name": "hermajan/sandbox",
	"description": "Pre-prepared Nette project for creation of website.",
	"type": "project",
	"license": "MIT",
	"homepage": "https://hermajan.net/sandbox",
	"support": {
		"issues": "https://github.com/hermajan/sandbox/issues"
	},
	"config": {
		"vendor-dir": "vendor",
		"platform-check": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"prefer-stable": true,
	"minimum-stability": "stable",
	"scripts": {
		"pre-install-cmd": [
			"composer check-platform-reqs"
		]
	},
	"require": {
		"php": "^7.3",
		"contributte/console": "^0.8.0",
		"contributte/logging": "^0.6.0",
		"contributte/translation": "^0.9.3",
		"doctrine/dbal": "^2.10",
		"doctrine/orm": "^2.7",
		"hermajan/dobine": "^0.3.0",
		"hermajan/fazette": "^0.3.0",
		"latte/latte": "^2.8",
		"nette/application": "^3.0",
		"nette/nette": "^3.0",
		"nettrine/annotations": "^0.6.2",
		"nettrine/orm": "^0.7.0",
		"nextras/mail-panel": "^2.5",
		"tracy/tracy": "^2.8",
		"vlucas/phpdotenv": "^5.1"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"phpstan/phpstan": "^0.12.33",
		"phpstan/phpstan-doctrine": "^0.12.19",
		"phpstan/phpstan-nette": "^0.12.8"
	},
	"suggest": {
		"geniv/nette-thumbnail": "Latte macro for creating image thumbnails."
	}
}
